<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo1.EditDialogController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Edit Quantity" />
        <HBox layoutX="14.0" layoutY="42.0" spacing="10.0">
            <Label text="Quantity:" />
            <TextField fx:id="quantityField" />
        </HBox>
        <Button layoutX="215.0" layoutY="105.0" mnemonicParsing="false" onAction="#saveChanges" text="Save" />
        <Button layoutX="134.0" layoutY="105.0" mnemonicParsing="false" onAction="#cancelEdit" text="Cancel" />
    </children>
</AnchorPane>
